{
    "name": "artisansdk/server",
    "description": "A service-based, Laravel PHP implementation of an async, realtime, WebSocket server.",
    "keywords": ["server", "laravel", "reactphp", "async", "realtime", "websocket", "socket", "process"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "cboden/ratchet": "~0.3.6",
        "illuminate/bus": "~5.4",
        "illuminate/contracts": "~5.4",
        "illuminate/config": "~5.4",
        "illuminate/container": "~5.4",
        "illuminate/queue": "~5.4",
        "illuminate/support": "~5.4",
        "ramsey/uuid": "~3.5",
        "react/child-process": "~0.4.3",
        "wyrihaximus/react-guzzle-psr7": "~2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.2",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "pdepend/pdepend": "~2.5.0",
        "phpmd/phpmd": "~2.6.0",
        "phpunit/phpunit": "~5.7",
        "sebastian/phpcpd": "~3.0.0"
    },
    "autoload": {
        "psr-4": {
            "ArtisanSDK\\Server\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ArtisanSDK\\Server\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": [
            "vendor/bin/php-cs-fixer fix -v"
        ],
        "test": [
            "vendor/bin/phpunit --colors=always",
            "vendor/bin/php-cs-fixer fix -v --diff --dry-run",
            "vendor/bin/phpcpd src/ config/ --min-lines=3 --min-tokens=35",
            "vendor/bin/phpmd \"src/,config/\" html codesize,cleancode,unusedcode,naming --reportfile reports/phpmd.html --ignore-violations-on-exit",
            "vendor/bin/pdepend --summary-xml=reports/stats.xml \"src/,config/\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
